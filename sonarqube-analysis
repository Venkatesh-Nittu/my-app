node{
    stage('SCM Checkout'){
        git 'https://github.com/Venkatesh-Nittu/my-app'
        }
    stage('Comple-Package'){
        def mhome = tool name: 'maven3', type: 'maven'
        sh "${mhome}/bin/mvn package"
    }
    stage('SonarQube Analysis') {
        def mhome = tool name: 'maven3', type: 'maven'
        withSonarQubeEnv('sonar') { 
          sh "${mhome}/bin/mvn sonar:sonar"
        }
}
